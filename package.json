{
  "name": "gitbook-plugin-linkz",
  "version": "1.0.0",
  "description": "\"Add links to your gitbook toolbar\"",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "npm run changelog && git add -A",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dnlup/gitbook-plugin-linkz.git"
  },
  "author": "dnlup <dwon.dnl@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dnlup/gitbook-plugin-linkz/issues"
  },
  "homepage": "https://github.com/dnlup/gitbook-plugin-linkz#readme",
  "engines": {
    "gitbook": ">2.x.x"
  },
  "gitbook": {
    "properties": {
      "icon": {
        "default": "fa fa-edit",
        "title": "Icon",
        "type": "string"
      },
      "label": {
        "default": "",
        "title": "Label",
        "type": "string"
      },
      "url": {
        "default": "/",
        "title": "URL",
        "type": "string"
      }
    }
  },
  "devDependencies": {
    "conventional-changelog-cli": "^2.2.2"
  }
}
